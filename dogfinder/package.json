{
  "name": "dogfinder",
  "version": "0.1.0",
  "private": true,
  "homepage": "http://bakrueger75.github.io/DogFinder-React-App",
  "dependencies": {
    "bootstrap": "^4.1.3",
    "jquery": "^1.9.1",
    "popper.js": "1.14.3",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-icons": "^3.2.1",
    "react-redux": "^5.0.7",
    "react-router": "latest",
    "react-router-dom": "latest",
    "react-router-redux": "^4.0.8",
    "react-scripts": "^1.1.0",
    "react-scroll": "^1.7.10",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-loader": "^8.0.4",
    "concat-glob-cli": "^0.1.0",
    "express": "^4.16.3",
    "gh-pages": "^2.0.1",
    "node-sass": "latest",
    "npm-run-all": "^4.1.3",
    "onchange": "latest",
    "webpack": "^4.20.2",
    "webpack-dev-middleware": "^3.4.0"
  },
  "scripts": {
    "prestart": "npm-run-all scss:compile",
    "start": "npm-run-all --parallel react:start scss:watch",
    "react:start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "scss:compile": "node-sass --source-comments -r ./src/css/scss/ -o ./src/css/compiled",
    "scss:watch": "npm run scss:compile -- --watch",
    "css:concat": "concat ./node_modules/bootstrap/dist/css/bootstrap.min.css ./src/css/compiled/dogfinder.css -o ./src/css/styles.css",
    "css:watch": "onchange \"./src/css/compiled/*.css\" -- npm run css:concat",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  }
}
