{
  "name": "dogfinder",
  "version": "0.1.0",
  "private": true,
  "homepage": "http://bakrueger75.github.io/DogFinder-React-Redux-App",
  "dependencies": {
    "bootstrap": "^4.1.3",
    "jquery": "^1.12.4",
    "popper.js": "1.14.3",
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "react-icons": "^3.2.2",
    "react-redux": "^5.1.0",
    "react-router": "latest",
    "react-router-dom": "latest",
    "react-router-redux": "^4.0.8",
    "react-scripts": "^1.1.5",
    "react-scroll": "^1.7.10",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.4",
    "css-loader": "^1.0.1",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^2.0.0",
    "gh-pages": "^2.0.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "ignore-loader": "^0.1.2",
    "mini-css-extract-plugin": "^0.4.4",
    "node-sass": "^4.10.0",
    "npm-run-all": "^4.1.3",
    "onchange": "^5.1.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.25.0",
    "webpack-cli": "^3.1.2",
    "webpack-dev-middleware": "^3.4.0",
    "webpack-dev-server": "^3.1.10"
  },
  "scripts": {
    "start": "npm-run-all --parallel webpack:start ",
    "react:start": "react-scripts start",
    "webpack:start": "webpack-dev-server --config config/webpack.config.js --open --mode development",
    "build": "npm run webpack:build",
    "react:build": "react-scripts build",
    "webpack:build": "webpack --mode production --config config/webpack.config.js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "scss:compile": "node-sass --source-comments -r ./src/css/scss/ -o ./src/css/compiled",
    "scss:watch": "npm run scss:compile -- --watch",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  }
}
